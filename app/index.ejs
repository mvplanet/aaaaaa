<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="utf-8">
	<title>QuickeeCam</title>
	<meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" />

	<link rel="stylesheet" type="text/css" href="./assets/css/bootstrap.min.css">
	<link rel="stylesheet" type="text/css" href="./assets/css/font-awesome.min.css">
	<link rel="stylesheet" type="text/css" href="./assets/css/csshake.min.css">
	<link rel="stylesheet" type="text/css" href="./assets/css/cam.css">

	<link rel="icon" type="image/png" href="./assets/favicon/icon_16.png" sizes="16x16">
	<link rel="shortcut icon" href="./assets/favicon/favicon.ico">

	<!-- Load JS -->
	<script src="./assets/js/jquery-1.11.3.min.js" type="text/javascript"></script>
	<script>
		window.jQuery = window.$ = module.exports;
		window.qcStartUpParams = {
			pageName: 'index',
			productEnv: <%= productEnv %>,
			asarPack: '<%= asarPack %>',
			configPath: '<%= configPath %>',
			electronBasePath: '<%= electronBasePath %>',
			appDataPath: '<%= appDataPath %>',
			quickStart: <%= quickStart %>
		};
	</script>
	<script src="./assets/js/bootstrap.min.js" type="text/javascript"></script>

	<script src="./assets/js/qrcode.min.js" type="text/javascript"></script>
	<script src="./assets/js/jquery.rotate.min.js" type="text/javascript"></script>
	<script src="./assets/js/jquery.tmpl.min.js" type="text/javascript"></script>
	<script src="./assets/js/jquery.events.js" type="text/javascript"></script>

	<script src="./assets/brfv4/libs/createjs/preloadjs-0.6.2.min.js"></script>

<% if (productEnv) { %>
	<script src="./app.min.js" type="text/javascript"></script>
<% } else { %>
	<script src="../src/app.header.js" type="text/javascript"></script>
	<script src="../src/config.js" type="text/javascript"></script>
	<script src="../src/log.js" type="text/javascript"></script>
	<script src="../src/misc.js" type="text/javascript"></script>
	<script src="../src/loader.js" type="text/javascript"></script>
	<script src="../src/session.js" type="text/javascript"></script>
	<script src="../src/timer.js" type="text/javascript"></script>
	<script src="../src/ffmpeg.js" type="text/javascript"></script>
	<script src="../src/network.js" type="text/javascript"></script>
	<script src="../src/file.js" type="text/javascript"></script>
	<script src="../src/webcam.js" type="text/javascript"></script>
	<script src="../src/framework.js" type="text/javascript"></script>
	<script src="../src/scene.js" type="text/javascript"></script>
	<script src="../src/event.js" type="text/javascript"></script>
	<script src="../src/uploader.js" type="text/javascript"></script>
	<script src="../src/qrcode.js" type="text/javascript"></script>
	<script src="../src/brfv4.js" type="text/javascript"></script>
	<script src="../src/app.js" type="text/javascript"></script>
<% } %>
</head>

<body class="webQC">
<div id="initial-page" class="scene" data-load-page="sceneInitialing.html" data-page-name="init"></div> 
<div id="cam-page" class="scene" data-load-page="sceneCam.html" data-page-name="webcam"></div>
<div id="relaunch-page" class="scene" data-load-page="appRelaunch.html" data-page-name="relaunch"></div>

<video id="localVideo" autoplay="autoplay" muted></video>
<canvas id="videoPreview"></canvas>
</body>
</html>
